@model MultiPlug.Base.Http.EdgeApp

<section class="row-fluid">
    <div class="row-fluid">
        <div class="box">
            <div class="span2">
                <a href="@Raw(Model.Context.Paths.Home)"><img alt="Network Icon" src="@Raw(Model.Context.Paths.Assets)images/networks.png"></a>
            </div>
            <div class="span8">
                <p style="font-size:22.75px; padding-top:10px; text-align:center;">Hypertext Transfer Protocol</p>
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="span6">
            <h3 class="box-header"><i class="icon-signout"></i>Clients [ 0 ]</h3>
            <div class="box non-collapsible">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>URL</th>
                            <th><a class="btn btn-green" href="@Raw(Model.Context.Paths.Home)httpclient/"><i class="icon-time icon-plus"></i></a></th>
                        </tr>
                    </thead>
                    <tbody>

                    </tbody>
                </table>
            </div>
        </div>
        <div class="span6">
            <h3 class="box-header"><i class="icon-signin"></i>Endpoints [ 0 ]</h3>
            <div class="box non-collapsible">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>URL</th>
                            <th><a class="btn btn-green" href="@Raw(Model.Context.Paths.Home)httpendpoint/"><i class="icon-time icon-plus"></i></a></th>
                        </tr>
                    </thead>
                    <tbody>

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>
